<%= form_for @booking do |f| %>
  <div class="form-group">
    <%= f.label :Wybierz samochód %>
    <%= select_tag(:car_id), class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Imię %>
    <%= f.text_field :last_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Nazwisko %>
    <%= f.text_field :first_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :'Adres e-mail' %>
    <%= f.text_field :first_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Telefon %>
    <%= f.text_field :first_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Wyjazd %>
    <%= f.text_field :first_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Powrót %>
    <%= f.text_field :first_name, class: "form-control" %>
  </div>
<% end %>
